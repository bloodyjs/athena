{
  "author": "Quildreen \"Sorella\" Motta <quildreen@gmail.com>",
  "name": "athena",
  "description": "Strategically gives you awesome functional combinators for an abstraction war!",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/killdream/athena.git"
  },
  "main": "./lib/index.js",
  "scripts": {
    "cc": "./node_modules/.bin/lsc -o lib -c src/*.ls",
    "test-cc": "./node_modules/.bin/lsc -o build/lib -c src/*.ls && ./node_modules/.bin/lsc -o build/test -c test/*.ls && ./node_modules/.bin/lsc -o build/test/specs -c test/specs/*.ls",
    "pretest": "npm run-script cc && npm run-script test-cc",
    "test": "node ./build/test/node.js",
    "prepublish": "npm run-script cc"
  },
  "dependencies": {},
  "devDependencies": {
    "LiveScript": "~1.1.1",
    "brofist-tap": "~0.1.0",
    "chai": "~1.6.0",
    "brofist": "~0.2.2",
    "brofist-minimal": "~0.1.1",
    "claire": "~0.3.4"
  },
  "optionalDependencies": {},
  "license": "MIT",
  "readmeFilename": "README.md",
  "calliope": {
    "output": "docs/api",
    "copyright": "Â© 2013 Quildreen \"Sorella\" Motta",
    "packages": [
      {
        "files": [
          "src/*.ls"
        ]
      }
    ]
  },
  "testling": {
    "files": "build/test/tap.js",
    "browsers": [
      "ie/6..latest",
      "chrome/20..latest",
      "firefox/15..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6"
    ]
  },
  "keywords": [
    "functional",
    "fp",
    "higher-order",
    "curry",
    "partial",
    "logic",
    "lambda"
  ]
}
